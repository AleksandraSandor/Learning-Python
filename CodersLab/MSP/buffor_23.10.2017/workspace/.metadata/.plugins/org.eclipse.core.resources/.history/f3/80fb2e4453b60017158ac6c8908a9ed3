print("Hello World")
a = "to jest tekst do testu"
print(a)
print(a.title())
print(a.upper())
print(a.lower())

mac = "to ta ta to"
mac_upper = mac.upper()
print(mac_upper)
print(mac)

a = "     sratatata     r  "
print(a)
print(a.rstrip())
print(a.lstrip())
print(a.strip())

a = ["a", "b", "c", "d"]
print(a)
a.append("e")
print(a)
a.insert(3, "e")
print(a)
del a[3]
print(a)
a.pop(0)
print(a)
a.remove("b")