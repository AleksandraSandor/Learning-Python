from random import randint

def Podaj_liczbe():
    test = False
    while(test == False):    
        try:        
            liczba = int(input("Zgadnij liczbę:"))
            test = True            
        except ValueError:
            print("Podałeś stringa zamiast liczby")    
    if(test == True):
        return(str(liczba)) 
    

a = Podaj_liczbe()
print(a)





