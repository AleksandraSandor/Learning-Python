def podaj_liczby():
    test = False
    test_2 = 0
    while(test == False):
        test_2 = 0         
        try:
            a = input("Podaj 6 liczb do losowania oddzielone spacją:")
            a = a.split()
            for i in range(0,5):
                if((int(a[i]) >= 1) and (int(a[i]) <= 49)):
                    print("Liczby są w zakresie")
                    continue
                else:
                    break
                j = i 
                for j in range(i, 5):
                    if(int(a[i]) == (int(a[j]))):
                       test_2 += 1
                       #print(test_2)
            if((test_2 == 5) and (test_2 > 0)):
                test = True
            else:
                print("Każda z liczb musi być inna")                     
            print(a)
                
            

               
             
        except ValueError:
            print("Jedna dana którą podałeś nie jest liczbą")
        except IndexError:
            print("Nie podałeś wszystkich liczb")
        #except Exception
            
podaj_liczby()




