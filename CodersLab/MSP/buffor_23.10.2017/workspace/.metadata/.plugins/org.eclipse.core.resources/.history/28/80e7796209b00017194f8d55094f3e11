from random import randint

def Podaj_liczbe():
    test = False
    liczba = 0
    while(test == False):    
        try:        
            liczba = int(input("Zgadnij liczbę:"))
            test = True            
        except ValueError:
            print("Podałeś stringa zamiast liczby")
        if((liczba < 1) or (liczba > 100)):
            print("Podałeś liczbę z poza zakresu")
            test = False    
        if(test == True):
            return(str(liczba)) 
    



wylosowana = randint(1, 100)
