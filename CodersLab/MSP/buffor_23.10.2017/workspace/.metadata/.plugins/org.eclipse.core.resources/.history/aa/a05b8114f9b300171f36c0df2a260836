class Auto:
    name = "Honda"
    __silnik = "v6"
    def __init__(self):
        print("Nowe auto")
    def print_info(self):
        print(self.__silnik)
        print(self)
    def __str__(self):
        return("Nazwa auto " + self.name)

a1 = Auto()
a2 = Auto()

# print(a1.name)
# print(a2.name)
print(__Auto__silnik)         #<--- DLACZEGO TO NIE DZIAŁA !!!
