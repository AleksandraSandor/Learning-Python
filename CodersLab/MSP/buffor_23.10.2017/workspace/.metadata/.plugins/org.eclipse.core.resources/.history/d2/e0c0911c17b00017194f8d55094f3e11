def podaj_liczby():
    test = False
    while(test == False):
        a = input("Podaj 6 liczb do losowania oddzielone spacją:")
        a = a.split()
        try:
            for i in range(0,5):
                if((int(a[i]) >= 1) and (int(a[i]) <= 49)):
                    print("test")
                    if a[i] not in a:
                        test = True
                
            

               
             
        except ValueError:
            print("Jedna dana którą podałeś nie są liczbą")
        except IndexError:
            print("Nie podałeś wszystkich liczb")
        #except Exception
            
podaj_liczby()




