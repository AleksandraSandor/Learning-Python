from random import randint

def Podaj_liczbe():
    test = False
    while(test == False):    
        try:        
            liczba = int(input("Zgadnij liczbę:"))
            test = True            
        except ValueError:
            print("Podałeś stringa zamiast liczby")
    if ((liczba < 1) or (liczba > 100)):
        test = False    
    if(test == True):
        return(str(liczba)) 
    

a = Podaj_liczbe()


wylosowana = randit(1,100)





