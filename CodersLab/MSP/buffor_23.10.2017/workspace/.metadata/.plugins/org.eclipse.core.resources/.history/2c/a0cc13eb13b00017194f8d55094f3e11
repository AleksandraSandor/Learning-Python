def podaj_liczby():
    test = False
    while(test == False):
        try:
            a1 = int(a[0])
            a2 = int(a[1])
            a3 = int(a[2])
            a4 = int(a[3])
            a5 = int(a[4])
            print("ok") 
        except ValueError:
            print("Jedna dana którą podałeś nie są liczbą")
        except IndexError:
            print("Nie podałeś wszystkich liczb")
        #except Exception
            
podaj_liczby()


