def jedi_generator():
    yield 1
    yield 2
    yield 3
    yield 4
    
jedi = jedi_generator()
jedi.__iter__()

print(jedi.__next__())
print(jedi.__next__())
print("test test")
print(jedi.__next__())
print(jedi.__next__())

jedi = jedi_generator()
jedi.__iter__()    
print(jedi.__next__())
    
    
    
    
