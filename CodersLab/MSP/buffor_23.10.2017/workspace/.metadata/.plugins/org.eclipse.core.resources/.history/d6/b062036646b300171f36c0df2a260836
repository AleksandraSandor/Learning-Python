import re
pattern = "ab*"
pattern = "ab+"
pattern = "ab?"
pattern = "ab{3}"
pattern = "ab{2}"                   
pattern = "[a-z]+_[a-z]+"               #6
pattern = "[A-Z]{1}[a-z]+"              #7
pattern = "^a.*b$"                      #8
pattern = "^\w"                     #9
# pattern = "\w+$"                    #10
# pattern = "\w+[z]\w+"               #11
# pattern = "\d+\D+"                  #13
# pattern = "\.0\d+"                  #14
# pattern = "\d+$"                    #15
# pattern = "[\d{1,3}]+"              #16
text = "aEExercises number anderb 1, 12, 13, and 345 666 are Importantb" #"Nie rozumiem tego 196" #
x = []
x = re.findall(pattern, text)

print(x)

