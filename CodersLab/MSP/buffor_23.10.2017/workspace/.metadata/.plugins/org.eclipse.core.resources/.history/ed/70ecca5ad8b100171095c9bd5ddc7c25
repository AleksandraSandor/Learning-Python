lista = [1, 2, "abc", " ", "10.10"]
tupla = (9, 8, "def", '"', "99.99")


def find_first_and_last(lista_lub_tupla):
    pierwszy = lista_lub_tupla[0]
    ostatni = lista_lub_tupla[len(lista_lub_tupla) - 1]
    #nowa_tupla = ()
    lista = []
    lista.append(pierwszy)
    lista.append(ostatni)
    nowa_tupla = tuple(lista)
    return(nowa_tupla)

print(find_first_and_last(lista))


